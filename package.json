{
  "author": "xinhao128",
  "bugs": {
    "url": "https://github.com/xinhao128/expo-nordic-dfu/issues"
  },
  "description": "Nordic DFU for Expo",
  "devDependencies": {
    "@types/react": "~19.0.10",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "expo": "^53.0.20",
    "expo-doctor": "^1.12.5",
    "expo-module-scripts": "^4.0.5",
    "prettier": "^3.4.2",
    "react-native": "0.79.5"
  },
  "expo-module": {
    "platforms": [
      "ios",
      "android"
    ]
  },
  "homepage": "https://github.com/xinhao128/expo-nordic-dfu#readme",
  "keywords": [
    "android",
    "dfu",
    "expo",
    "expo-module",
    "ios",
    "nordic",
    "react-native"
  ],
  "license": "MIT",
  "main": "build/index.js",
  "name": "@xinhao128/expo-nordic-dfu",
  "peerDependencies": {
    "expo": "^53.0.20",
    "react": "^19.0.0",
    "react-native": "^0.79.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "react-native": {
    "codegenConfig": {
      "jsSrcsDir": "src",
      "libraryName": "ExpoNordicDfu",
      "name": "ExpoNordicDfu",
      "type": "modules"
    }
  },
  "repository": "xinhao128/expo-nordic-dfu",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "expo-module": "expo-module",
    "eslint:check": "eslint . --max-warnings 0",
    "eslint:fix": "eslint . --fix",
    "open:android": "open -a \"Android Studio\" example/android",
    "open:ios": "xed example/ios",
    "prettier:check": "prettier --config .prettierrc --check '**/*.{ts,tsx,js,jsx}'",
    "prettier:fix": "prettier --config .prettierrc --write '**/*.{ts,tsx,js,jsx}'",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "test": "expo-module test"
  },
  "types": "build/index.d.ts",
  "url": "https://github.com/xinhao128/expo-nordic-dfu",
  "version": "1.0.0"
}
